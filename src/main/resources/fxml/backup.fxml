<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.aizen.controller.BackupController">
    <ListView fx:id="list" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10.0" AnchorPane.bottomAnchor="50"
              AnchorPane.leftAnchor="10"/>
    <Button fx:id="restore" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10"
            prefWidth="60" prefHeight="25"
            mnemonicParsing="false" text="Restore"/>
    <Button fx:id="delete" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="10"
            prefWidth="60" prefHeight="25" mnemonicParsing="false" text="Delete"/>
</AnchorPane>
